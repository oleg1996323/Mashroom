{
    "configurations": [
    {
        "name": "(gdb) Launch",
        "type": "cppdbg",
        "request": "launch",
        "program": "/home/oster/Mashroom/bin/Debug/Mashroom",
        "args": ["-p","/home/oster/grib_files/","-j","4"],
        "stopAtEntry": false,
        "cwd": "${cwd}",
        "externalConsole": false,
        "MIMode": "gdb",
        "miDebuggerPath": "/home/oster/binutils-gdb/gdb-install/bin/gdb",
        "environment": [
            {
                "name": "LD_LIBRARY_PATH",
                "value": "/home/oster/gcc/gcc-install/lib64"
            }
        ],
        "setupCommands": [
            {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            },
            {
                "description": "Set Disassembly Flavor to Intel",
                "text": "-gdb-set disassembly-flavor intel",
                "ignoreFailures": true
            }
        ]
    }
    ]
}